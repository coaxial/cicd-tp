{
  "name": "my-project",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "lint": "eslint .",
    "test": "jest",
    "test:unit": "jest tests/unit --reporters=default --reporters=jest-junit",
    "test:integration": "jest tests/integration --reporters=default --reporters=jest-junit",
    "test:e2e": "jest tests/e2e --reporters=default --reporters=jest-junit",
    "start": "node src/server.js"
  },
  "dependencies": {
    "axios": "^1.3.4",
    "express": "^4.18.2",
    "supertest": "^6.3.0"
  },
  "devDependencies": {
    "allure-jest": "^3.4.3",
    "eslint": "^8.0.0",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0"
  },
  "engines": {
    "node": ">=22.19.0"
  },
  "jest-junit": {
  "outputDirectory": "junit-results",
  "outputName": "junit.xml"
  },
  "files": [
    ".gitignore",
    "README.md",
    "src/index.js",
    "tests/unit/hello.test.js",
    "tests/integration/hello.integration.test.js",
    "tests/e2e/hello.e2e.test.js"
  ]
}
